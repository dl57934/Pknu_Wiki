<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <display-name>Apache-Axis</display-name>
    <servlet>
        <display-name>Apache-Axis Servlet</display-name>
        <servlet-name>AxisServlet</servlet-name>
        <servlet-class>org.apache.axis.transport.http.AxisServlet</servlet-class>
    </servlet>
    <servlet>
        <display-name>Axis Admin Servlet</display-name>
        <servlet-name>AdminServlet</servlet-name>
        <servlet-class>org.apache.axis.transport.http.AdminServlet</servlet-class>
        <load-on-startup>100</load-on-startup>
    </servlet>
    <servlet>
        <display-name>SOAPMonitorService</display-name>
        <servlet-name>SOAPMonitorService</servlet-name>
        <servlet-class>org.apache.axis.monitor.SOAPMonitorService</servlet-class>
        <init-param>
            <param-name>SOAPMonitorPort</param-name>
            <param-value>5101</param-value>
        </init-param>
        <load-on-startup>100</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>AxisServlet</servlet-name>
        <url-pattern>/servlet/AxisServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AxisServlet</servlet-name>
        <url-pattern>*.jws</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AxisServlet</servlet-name>
        <url-pattern>/services/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SOAPMonitorService</servlet-name>
        <url-pattern>/SOAPMonitor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminServlet</servlet-name>
        <url-pattern>/servlet/AdminServlet</url-pattern>
    </servlet-mapping>
    <mime-mapping>
        <extension>wsdl</extension>
        <mime-type>text/xml</mime-type>
    </mime-mapping>
    <servlet>
        <servlet-name>main</servlet-name>
        <jsp-file>/PknuWiki/view/main.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>main</servlet-name>
        <url-pattern>/PknuWiki/view/main</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>view</servlet-name>
        <jsp-file>/PknuWiki/view/view.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>view</servlet-name>
        <url-pattern>/PknuWiki/view/view</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>code</servlet-name>
        <jsp-file>/PknuWiki/view/code.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>code</servlet-name>
        <url-pattern>/PknuWiki/view/code</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>login</servlet-name>
        <jsp-file>/PknuWiki/view/login.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/PknuWiki/view/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>modifyView</servlet-name>
        <jsp-file>/PknuWiki/view/modifyView.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>modifyView</servlet-name>
        <url-pattern>/PknuWiki/view/modifyView</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>overlapView</servlet-name>
        <jsp-file>/PknuWiki/view/overlapView.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>overlapView</servlet-name>
        <url-pattern>/PknuWiki/view/overlapView</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>preview</servlet-name>
        <jsp-file>/PknuWiki/view/preview.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>preview</servlet-name>
        <url-pattern>/PknuWiki/view/preview</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>signUp</servlet-name>
        <jsp-file>/PknuWiki/view/signUp.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>signUp</servlet-name>
        <url-pattern>/PknuWiki/view/signUp</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>writing</servlet-name>
        <jsp-file>/PknuWiki/view/writing.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>writing</servlet-name>
        <url-pattern>/PknuWiki/view/writing</url-pattern>
    </servlet-mapping>
</web-app>